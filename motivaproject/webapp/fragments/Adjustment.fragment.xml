<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:u="sap.ui.unified">
	<Dialog xmlns="sap.m" id="__createDialog" busy="false" busyIndicatorDelay="1000" class="sapUiSizeCompact" visible="true" icon=""
		title="Add Adjustment MF" contentWidth="auto" contentHeight="auto">
		<dependents/>
		<!-- sap.ui.core.Control, since 1.19 -->
		<content>
			<f:SimpleForm id="SimpleFormDisplay480_Trial_opy" editable="false" layout="ResponsiveGridLayout" labelSpanL="3" labelSpanM="3" columnsL="2" columnsM="2" singleContainerFullSize="false">
				<f:content>
                    <Label id="_IDGenLabel1" text="Select" required="true"/>
					<Select id="_IDGenSelect1" >
                        <core:Item id="_IDGenItem1" key="" text="" />
                        <core:Item id="_IDGenItem2" key="1" text="Correction from Prior Period" />
                        <core:Item id="_IDGenItem3" key="2" text="Customer Charge Tax in Error" />
                        <core:Item id="_IDGenItem4" key="3" text="Customer Not Charged Tax in Error" />
                        <core:Item id="_IDGenItem5" key="4" text="Prior Period Transaction" />
                        <core:Item id="_IDGenItem6" key="5" text="Quarterly return" />
                        <core:Item id="_IDGenItem7" key="6" text="Other" />
                    </Select>
                    <Label id="_IDGenLabel2" text="Add description"/>
                    <TextArea id="textArea1"/>
                    <Label id="_IDGenLabel3" text="Fuel Types"/>
                    <VBox id="_IDGenVBox1" class="sapUiSmallMargin">
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData1" growFactor="1" shrinkFactor="0" /> <!-- Don't shrink below minimum size. Fill space if available. -->
					</layoutData>
                    <CheckBox id="_IDGenCheckBox1" text="Gasoline" select="onCheckBox1"></CheckBox>
                    <Label id="_IDGenLabel4"  text="Rate" visible="false"/>
                    <Input id="_IDGenInput1" visible="false" width="25%"/>
                    </VBox>
                    <VBox id="_IDGenVBox2" class="sapUiSmallMargin">
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData2" growFactor="1" shrinkFactor="0" /> <!-- Don't shrink below minimum size. Fill space if available. -->
					</layoutData>
                    <CheckBox id="_IDGenCheckBox2" text="Diesel" select="onCheckBox2"></CheckBox>
                    <Label id="_IDGenLabel5"  text="Rate" visible="false"/>
                    <Input id="_IDGenInput2" visible="false" width="25%"/>
                    </VBox>
                    <VBox id="_IDGenVBox3" class="sapUiSmallMargin">
					<layoutData>
						<FlexItemData id="_IDGenFlexItemData3" growFactor="1" shrinkFactor="0" /> <!-- Don't shrink below minimum size. Fill space if available. -->
					</layoutData>
                    <CheckBox id="_IDGenCheckBox3" text="Special Fuel" select="onCheckBox3"></CheckBox>
                    <Label id="_IDGenLabel6"  text="Rate" visible="false"/>
                    <Input id="_IDGenInput3" visible="false" width="25%"/>
                    </VBox>
                    <Label id="_IDGenLabel11" text="Category" required="true"/>
					<Select id="_IDGenSelect11" >
                        <core:Item id="_IDGenItem11" key="" text="" />
                        <core:Item id="_IDGenItem21" key="1" text="Required Adjustment" />
                        <core:Item id="_IDGenItem31" key="2" text="List Component" />
                        <core:Item id="_IDGenItem41" key="3" text="Timing Item" />
                    </Select>
                    <Label id="_IDGenLabel21" text="State"/>
                    <Select id="_IDGenSelect21" >
                        <core:Item id="_IDGenItem12" key="" text="" />
                        <core:Item id="_IDGenItem22" key="1" text="Alabama" />
                        <core:Item id="_IDGenItem32" key="2" text="Texas" />
                        <core:Item id="_IDGenItem42" key="3" text="Georgia" />
                    </Select>
				</f:content>
			</f:SimpleForm>
		</content>
		<buttons>
			<Button id="_IDGenButton1" visible="true" text="Submit" type="Accept" iconDensityAware="true" textDirection="Inherit" press="submitCloseAdjustment"/>
			<Button id="_IDGenButton2" visible="true" text="Cancel" type="Reject" iconDensityAware="true" textDirection="Inherit" press="submitCloseAdjustment"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>
